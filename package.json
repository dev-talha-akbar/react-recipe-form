{
  "name": "react-recipe-form",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "i18n:scan": "npx tsc --jsx preserve --target ES6 --module es6 --noEmit false --allowImportingTsExtensions false --outDir ./build-i18n && npx i18next-scanner --config i18next-scanner.config.cts"
  },
  "dependencies": {
    "bootstrap": "^5.3.1",
    "bootstrap-icons": "^1.10.5",
    "i18next": "^23.4.4",
    "i18next-http-backend": "^2.2.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.1.2",
    "react-query": "^3.39.3"
  },
  "devDependencies": {
    "@types/node": "^20.5.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "i18next-scanner": "^4.3.0",
    "i18next-scanner-typescript": "^1.1.1",
    "sass": "^1.66.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
